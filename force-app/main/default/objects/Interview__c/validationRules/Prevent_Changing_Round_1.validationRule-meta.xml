<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Changing_Round_1</fullName>
    <active>true</active>
    <description>Prevent Changing R1_Status After It&apos;s Set to Fail</description>
    <errorConditionFormula>AND(
  ISPICKVAL(PRIORVALUE( Round_1_Coding__c ), &quot;Fail&quot;),
  NOT(ISPICKVAL(Round_1_Coding__c, &quot;Fail&quot;))
)</errorConditionFormula>
    <errorDisplayField>Round_1_Coding__c</errorDisplayField>
    <errorMessage>You cannot change Round 1 status after it is marked as Fail.</errorMessage>
</ValidationRule>
