<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Changing_Round_2</fullName>
    <active>true</active>
    <description>Prevent Changing  Round 2 After It&apos;s Set to Fail</description>
    <errorConditionFormula>AND(
  ISPICKVAL(PRIORVALUE( Round2_Technical_Interview__c ), &quot;Fail&quot;),
  NOT(ISPICKVAL(Round2_Technical_Interview__c, &quot;Fail&quot;))
)</errorConditionFormula>
    <errorDisplayField>Round2_Technical_Interview__c</errorDisplayField>
    <errorMessage>You cannot change Round 2 status after it is marked as Fail.</errorMessage>
</ValidationRule>
